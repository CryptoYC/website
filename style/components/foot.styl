.foot {
  padding: 100px 130px 100px;
  color: #fff;
  background: black;

 .title {
    height: 80px;
    font-size: 42px;
    margin-bottom: 20px;
    position: relative;
    a{
      img{
        top: 50%;
        position:absolute;
        transform: translateY(-36%);
      }
    }
  }

  .content {
    display: flex;
    flex-direction: column;
    min-height: 160px;
    justify-content: space-between;

    a {
      color: #fff;
      text-align: left;
          width: fit-content;
    }
  }
  button{
     color: #fff;
  }
  .logo {
    margin-top: 20px;
    color: #fff;
        // border-top: 1px solid #b7b5b5;
    padding-top: 20px;
     border-top: 1px solid #888686;
    .image {
          width: max-content;
    }
  }
  .icon_list{
    height: 100px;
    width: 200px;
    .icon_item{
      padding: 0px 10px }
    }
}